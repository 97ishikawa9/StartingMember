<h1>スターティングメンバー</h1>
<%= link_to "スタメンを発表する", new_member_path, class:"post" %>

<div class="contents row">
  <% @members.each do |member| %>
    <div class="content_post">
      <h1><%= member.team.name %></h1>
      <p>|1(<%= member.one_position.name %>) <%= member.one_name.name %>|2(<%= member.two_position.name %>) <%= member.two_name.name %>|3(<%= member.three_position.name %>) <%= member.three_name.name %>|4(<%= member.four_position.name %>) <%= member.four_name.name %>|5(<%= member.five_position.name %>) <%= member.five_name.name %>|6(<%= member.six_position.name %>) <%= member.six_name.name %>|7(<%= member.seven_position.name %>) <%= member.seven_name.name %>|8(<%= member.eight_position.name %>) <%= member.eight_name.name %>|9(<%= member.nine_position.name %>) <%= member.nine_name.name %>|</p>
    </div>
  <% end %>
</div>